#include<iostream>
using namespace std;

int main()
{
    int N;
    cin>>N;
    int a[N];
    int c,count=0;
    
    for(int i=0;i<N;i++){
     cin>>a[i];
    }
    
    for(int i=0;i<N;i++){
    
    c=a[i];
    
    for(int j=i+1; j<N;j++){
    
    if(c > a[j]){
     count++;
    }
    }
    }
    cout<<count;
    
    
    
    return 0;
}
